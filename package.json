{
  "name": "vessel-node",
  "version": "1.0.1",
  "description": "Vessel utilities",
  "types": "./types/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./module.mjs"
  },
  "files": [
    "dist/**/*",
    "types/**/*",
    "module.mjs"
  ],
  "repository": {
